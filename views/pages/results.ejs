  <body onload="generateEstimate()">

    <div class = 'top-left'>
      <a onclick="startQuiz()" class="giveCursor">
        <h4>< - Start Again</h4>
      </a>
    </div>

    <div id = "quote" class = 'top-right'>
      <button onclick="shareLink()" type="button" class="btn btn-lg btn--light" data-toggle="modal" data-target=".bd-example-modal-lg">Get Link to Save or Share<span class="fa fa-chevron-circle-right"></span></button>

<!-- Modal -->
      <div class="modal fade bd-example-modal-lg" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-lg">
          <div class="modal-content">
            <div class="text-center modal-header">
              <h5 class="modal-title" id="exampleModalLongTitle">Share Project Estimate</h5>
              <br />
              <p class="modal-title">
                Save this link to review your project later or share it with a friend.
              </p>
              <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span class="text-light fa fa-times fa-3x" aria-hidden="true"></span>
              </button>
            </div>
            <div class="modal-body">
              <br />
              <input class="form-text-area" type="text" value="Copy text!" id="shareLink">
            </div>
            <div class="modal-footer">
              <button class="btn btn-info" onclick="copyText()">Copy text</button>
            </div>
          </div>
        </div>
      </div>
    </div>

    <div class="text-center top-buffer bottom-buffer">
      <div class ="container-fluid">
        <div class="theme-dark text-center wow fadeInUp anim" data-wow-delay="0.3s">
          <h1>Your custom website estimate</h1>
            <img class="fadeInUp" src="../images/icons/code.svg" alt="How much does a website cost?">
            <h1><span class="estimate">€</span><span id = "quote" class="estimate final-quote">0</span><span class="estimate"> – </span><span class="estimate">€</span><span id = "quote" class="estimate final-quote">0</span></h1>
            <button onclick="breakdownReveal()" class="btn btn--light">Breakdown</button>



            <!-- BREAKDDOWN -->
            <div class="text-center" id="breakdownDIV">
              <div class="container">
                  <ul>
