                                <div class="pull-left">
                                    <img class="media-object" src="<%= results[i].imageURL %>" alt="" height="75" width="75">
                                </div>
                                <div class="media-body">
                                    <p><%= results[i].question %> </p>
                                    <p><%= results[i].answer %> </p>
                                    <p id=value style="display: none"><%= results[i].value %></p>



                                  <!--  <p>Answer: <span id="<% i %>"></span></p> -->
                                </div>
                            </div>
                    </li>
                    <%   } %>
